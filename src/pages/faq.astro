---
import PageLayout from '../layouts/PageLayout.astro';
import BreadcrumbHero from '../components/BreadcrumbHero.astro';
import FaqSection from '../components/FaqSection.astro';
import CtaThreeSection from '../components/CtaThreeSection.astro';
import { site } from '../data/site';
---

<PageLayout title={`FAQ | ${site.name}`} description="Find answers to common questions about our construction services, project timelines, pricing, and building process." headerLogoVariant="black" headerMenuHoverVariant="secondary" footerVariant="light">
  <BreadcrumbHero title="Frequently Asked Questions" />
  <FaqSection />
  <CtaThreeSection />
</PageLayout>

